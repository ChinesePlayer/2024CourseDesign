<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.teach.javafx.controller.teacherCourse.MyTaughtController">

    <center>
        <TableView fx:id="courseTableView" prefHeight="400.0" prefWidth="3000.0">
            <columns>

                <TableColumn fx:id="courseNum" minWidth="0.0" prefWidth="220.0" text="课序号" />
                <TableColumn fx:id="courseName" prefWidth="220" text="课程名称" />
                <TableColumn fx:id="days" minWidth="0.0" prefWidth="220.0" text="上课时间" />
                <TableColumn fx:id="sections" prefWidth="220.0" text="上课节次" />
                <TableColumn fx:id="loc" minWidth="0.0" prefWidth="300.0" text="上课地点" />
                <TableColumn fx:id="action" prefWidth="328" sortable="false" text="操作" />

            </columns>
        </TableView>
    </center>

</BorderPane>
